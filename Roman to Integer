class Solution(object):
    def romanToInt(self, s):
        """
        :type s: str
        :rtype: int
        """
        sum = 0
        Dict = {"I":1, "V":5, "X":10, "L":50, "C":100, "D":500, "M":1000}
        for i in range(len(s)):
            if i == len(s)-1 or Dict[s[i]] >= Dict[s[i+1]] :  
                sum += Dict[s[i]]
            else : 
                sum -= Dict[s[i]]
        return sum
